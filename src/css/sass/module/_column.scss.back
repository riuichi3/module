.c-column {
  text-align:center;
  //font-size:0; //inline-blockが持つスペースを無くす
  letter-spacing:-0.34em; // font-size:0;ではsafariでうまくいかない模様。。。
  &.-pad {
   padding-left :30px;
   padding-right:30px;
   @include mq_l() {
      margin-bottom:0;
      padding-left :100px;
      padding-right:100px;
   }
  }
  >.sub-cell {
    display:inline-block;
    vertical-align:top;
    text-align:left;
    letter-spacing:normal;//”inline-blockが持つ改行時のスペースを無くす”のリセット
    box-sizing:border-box;
    >.sub-inner {
      text-align:left;
    }
  }
  &.-vMiddle {
    >.sub-cell {
      vertical-align:middle;
    }
  }
  //グリッド下マージン
  margin-bottom:40px;
  &:last-child{
    margin-bottom:0;
  }
  //マージンdef
  $def_pc:20px;
  //column全体の上と左にネガティブマージン
  margin-top:-$def_pc;
  margin-left:-$def_pc;
  >.sub-cell {
    padding-top:$def_pc;
    padding-left:$def_pc;
    >.sub-inner {}
  }
  &.-marginS {
    //マージンS
    $s_pc:10px;
    //column全体の上と左にネガティブマージン
    margin-top:-$s_pc;
    margin-left:-$s_pc;
    >.sub-cell {
      padding-top:$s_pc;
      padding-left:$s_pc;
      >.sub-inner {}
    }
  }
  &.-marginL {
    //マージンL
    $l_pc:30px;
    //column全体の上と左にネガティブマージン
    margin-top:-$l_pc;
    margin-left:-$l_pc;
    >.sub-cell {
      padding-top:$l_pc;
      padding-left:$l_pc;
      >.sub-inner {}
    }
  }
  &.-marginZero {
    //マージン0
    $zero_pc:0px;
    //column全体の上と左にネガティブマージン
    margin-top:-$zero_pc;
    margin-left:-$zero_pc;
    >.sub-cell {
      padding-top:$zero_pc;
      padding-left:$zero_pc;
      >.sub-inner {}
    }
  }
  // ---------------------------------------
  //  矢印でつなぐ
  // ---------------------------------------
  &.-step {
    $step_pc:40px;
    margin-top:-$step_pc;
    margin-left:-$step_pc;
    >.sub-cell {
      padding-top:$step_pc;
      padding-left:$step_pc;
      >.sub-inner {
        position:relative;
        &:before {
          content:"→";
          display:block; width:1em; height:1em; line-height:1em;
          position:absolute; top:0; bottom:0; left:-1.8em; margin:auto;
        }
      }
      &.noArrow {
        >.sub-inner {
          position:static;
          &:before {
            display:none !important;
          }
        }
      }
      // ---------------------------------------
      //  斜め矢印
      // ---------------------------------------
      &.tiltArrow {
        margin:-$step_pc/2 0;
        >.sub-inner:before {
          display:none !important;
        }
        &+.sub-cell>.sub-inner:before {
          display:none !important;
        }
      }
    }
  }
}

@include mq_s() {
  .c-column {
    //グリッド下マージン
    margin-bottom:20px;
    &:last-child{
      margin-bottom:0;
    }
    //マージンdef
    $def_sp:10px;
    //column全体の上と左にネガティブマージン
    margin-top:-$def_sp;
    margin-left:-$def_sp;
    >.sub-cell {
      padding-top:$def_sp;
      padding-left:$def_sp;
      >.sub-inner {}
    }
    &.-marginS {
      //マージンS
      $s_sp:5px;
      //column全体の上と左にネガティブマージン
      margin-top:-$s_sp;
      margin-left:-$s_sp;
      >.sub-cell {
        padding-top:$s_sp;
        padding-left:$s_sp;
        >.sub-inner {}
      }
    }
    &.-marginL {
      //マージンL
      $l_sp:15px;
      //column全体の上と左にネガティブマージン
      margin-top:-$l_sp;
      margin-left:-$l_sp;
      >.sub-cell {
        padding-top:$l_sp;
        padding-left:$l_sp;
        >.sub-inner {}
      }
    }
    &.-marginZero {
      //マージン0
      $zero_sp:0px;
      //column全体の上と左にネガティブマージン
      margin-top:-$zero_sp;
      margin-left:-$zero_sp;
      >.sub-cell {
        padding-top:$zero_sp;
        padding-left:$zero_sp;
        >.sub-inner {}
      }
    }
    // ---------------------------------------
    //  矢印でつなぐ
    // ---------------------------------------
    &.-step {
      $step_sp:20px;
      margin-top:-$step_sp;
      margin-left:-$step_sp;
      >.sub-cell {
        padding-top:$step_sp;
        padding-left:$step_sp;
        >.sub-inner {
          position:relative;
          &:before {
            content:"→";
            display:block; width:1em; height:1em; line-height:1em;
            position:absolute; top:0; bottom:0; left:-1.2em; margin:auto;
          }
        }
      }

      // ---------------------------------------
      //  斜め矢印
      // ---------------------------------------
      >.sub-cell.tiltArrow {
        display:none;
        &+.sub-cell>.sub-inner:before {
          display:block !important;
        }
      }
      &.-sp1col.-step {
        $step_sp:20px;
        margin-top:-$step_sp;
        margin-left:-$step_sp;
        >.sub-cell {
          padding-top:$step_sp;
          padding-left:$step_sp;
          >.sub-inner {
            position:relative;
            &:before {
              content:"↓";
              display:block; width:1em; height:1em; line-height:1em;
              position:absolute; top:-1.2em; bottom:auto; left:0; right:0; margin:auto;
            }
          }
        }
      }
    }
    &.-sp1col >.sub-cell { width:100%; }
    &.-sp2col >.sub-cell { width:50%; }
    &.-sp3col >.sub-cell { width:33.3333%; }
    &.-sp4col >.sub-cell { width:25%; }
  }
}//@media sp
